//это коментарий
/*
это тоже коментарий
*/
{
  "version": 1.1,
  "name": "example",
  "description": "example",
  //пол - есть три два: мальчик (BOY), девочка(GIRL)
  "sex": "BOY",
  //сложность - есть три варианта: простой (EASY), нормальный(NORMAL), сложный (HARD)
  "complexity": "NORMAL",
  "levels": [
    {
      //новый параметр: отложенный запуск задачи
      "delayedStart": true,
      //название уровня (необязательно, но будем его отображать)
      "name": "name_level_0",
      //описание уровня (необязательно, но можем его отображать)
      "description": "Прокачай пиздюка",
      //сколько уровень весит в очках
      "pointsWeight": 10000,
      //множитель очков: конечное количество очков, которое расчитывается исходя из награды за правильное решения задания умноженое на множитель очков
      "pointsMultiplier": 1.0,
      //правила для заданий
      "quests": [
        //МОНЕТЫ (solution, unknown_member)
        {
          "questType": "монеты",
          "memberCount": 4,
          //возможные варианты: solution или unknown_member (в данном случае незвестный член - unknown_member)
          //можно сразу несколько указывать
          "questionTypes": [
            "unknown_member",
            "solution"
          ],
          //награда (может быть и не указана, по умолчанию 10)/
          "reward": 10
        },
        //СВЕТОФОР (solution)
        {
          "questType": "светофор",
          "enabled": false
        },
        //ВРЕМЯ (unknown_member)
        {
          "questType": "время",
          "reward": 10,
          "questionTypes": [
            "unknown_member",
            "comparison"
          ],
          "enabled": false,
          //количестко часов среди которых нужно найти правильный ответ (необязательно, будут подставлены значения по умолчанию)
          "memberCount": 4
        },
        //ВЫБОР (unknown_member)
        {
          "questType": "выбор",
          "reward": 10,
          //Название наборов из которых делать выбор, на данный момент есть одна группа - это сезоны или впемена года
          "types": [
            "seasons",
            "berry",
            "pomum"
          ],
          "enabled": false
        },
        //ФРУКТЫ (solution)
        {
          //название задания. Варианты: фрукты, время, монеты, выбор
          "questType": "фрукты",
          //возможные варианты: solution или comparison (в данном случае решение).
          "questionTypes": [
            "solution"
          ],
          //количество членов в примере (в данном случае 2 получается a + b) (необязательно, будут подставлены значения по умолчанию)
          "memberCount": 2,
          //минимальное и максимальное значение для каждого члена в примере (необязательно, будут подставлены значения по умолчанию)
          //вначале минимальные значения по порядку, потом максимальные по порядку
          "memberMinAndMaxValues": [
            [
              1,
              1
            ],
            [
              5,
              5
            ]
          ],
          "enabled": false
        }
      ]
    }
  ]
}